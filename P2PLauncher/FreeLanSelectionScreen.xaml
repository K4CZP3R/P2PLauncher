<Window x:Class="P2PLauncher.FreeLanSelectionScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:P2PLauncher"
        mc:Ignorable="d"
        Title="FreeLanSelectionScreen" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <Label x:Name="StatusDescription" Content="Label" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TabControl x:Name="freeLanTabs" HorizontalAlignment="Left" Height="292" Margin="10,117,0,0" VerticalAlignment="Top" Width="772">
            <TabItem Header="Locate it">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Download">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
